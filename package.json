{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production remix build",
    "remix:dev": "cross-env NODE_ENV=development remix dev",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production remix-serve build",
    "typecheck": "tsc --noEmit --pretty",
    "typecheck:watch": "npm run typecheck -- --watch --preserveWatchOutput",
    "clean": "rm -rf node_modules",
    "clean:dev": "npm run clean && npm i && npm run dev",
    "dev": "concurrently -k -p \"-{index}_{name} \" npm:typecheck:watch npm:remix:dev",
    "image:build": "docker build -f ../deployment/remix-test/Dockerfile -t us-central1-docker.pkg.dev/development-262721/draw-draw-ink-client/ddi-client:\"$(git describe)\" .",
    "image:promote": "docker push us-central1-docker.pkg.dev/development-262721/draw-draw-ink-client/ddi-client:\"$(git describe)\"",
    "image:start": "CLIENT_APP_IMAGE_NAME=us-central1-docker.pkg.dev/development-262721/draw-draw-ink-client/ddi-client:\"$(git describe)\" docker-compose -f ./docker-compose-image-test.yml up",
    "image:bash": "docker-compose -f ./docker-compose-image-test.yml exec ddi-client /bin/sh",
    "publish": "npm run image:build && npm run image:promote",
    "publish:test": "npm version prerelease --preid=g\"$(git rev-parse --short=5 HEAD)\" && npm run publish",
    "publish:patch": "npm version patch && npm run publish",
    "publish:minor": "npm version minor && npm run publish",
    "publish:major": "npm version major && npm run publish"
  },
  "dependencies": {
    "@remix-run/react": "^1.2.3",
    "@remix-run/serve": "^1.2.3",
    "classnames": "^2.3.1",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "newrelic": "^8.11.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-image": "^4.0.3",
    "react-page-visibility": "^7.0.0",
    "remix": "^1.2.3",
    "store2": "^2.13.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "@types/newrelic": "^7.0.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-page-visibility": "^6.4.1",
    "@types/styled-components": "^5.1.24",
    "eslint": "^8.9.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  },
  "version": "0.0.1-g003b0.0"
}
